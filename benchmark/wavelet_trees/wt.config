WT_HUTU;wt_hutu<bit_vector, rank_support_v<>, select_support_mcl<1>, select_support_mcl<0>, int_tree<>>;WT-HUTU
WT_INT;wt_int<>;WT-INT
WT_INT_RRR_15;wt_int<rrr_vector<15>>;WT-INT-RRR-15
WT_INT_RRR_63;wt_int<rrr_vector<63>>;WT-INT-RRR-63
